Private Sub Command1_Click()
Dim i As Integer For i = 0 To List1.ListCount - 1 
If List1.Selected(i) = True Then 
List2.AddItem (List1.List(i))
Exit For
End If
Next
End Sub

Private Sub Command2_Click()
Dim i As Integer For i = 0 To List1.ListCount - 1 
If List1.Selected(i) = True Then 
List1.RemoveItem (i)
Exit For

End If
Next
End Sub

Private Sub Command3_Click() 
Dim i As Integer For i = 0 To List2. ListCount - 1 
If List2.Selected(i) = True Then 
List2.Removeltem (i)
Exit For
End If
Next
End Sub

Private Sub Command4_Click()
List1.Clear
List2.Clear
End Sub

Private Sub Command5_Click()
End
End Sub
